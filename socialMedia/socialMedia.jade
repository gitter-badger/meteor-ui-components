template(name="socialMedia")
  .socialMedia.s-grid-stretch.text-center
    .s-grid-cell-sm-12
      h1 Let's be friends
    .s-grid-cell-sm-12
      p We're on all your favorite social networks.  Come say hello!
    .iconGroup
      a(href="http://facebook.com/{{ facebookURL }}")
        +icon(face="facebook")
      a(href="http://twitter.com/{{ twitterURL }}")
        +icon(face="twitter")
      a(href="http://instagram.com/{{ instagramURL }}")
        +icon(face="instagram")
