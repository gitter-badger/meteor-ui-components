template(name="socialMedia")
  .socialMedia.s-grid-stretch.text-center
    .s-grid-cell-sm-12
      h1 Let's be friends
    .s-grid-cell-sm-12
      p We're on all your favorite social networks.  Come say hello!
    .s-grid-cell-sm-4
      +icon(face="facebook")
    .s-grid-cell-sm-4
      +icon(face="twitter")
    .s-grid-cell-sm-4
      +icon(face="instagram")
